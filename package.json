    "node": "^16.* || >= 18"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "^2.3.5",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@vitest/coverage-c8": "^0.26.2",
    "eslint": "^8.30.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "pnpm": {
